    "@types/node": "^20.12.12",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
